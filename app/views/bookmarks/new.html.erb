<div class="cadre-form">
  <h1>Choisir et ajouter un film</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.association :movie, label: "Choisir un film" %>
    <%= f.input :comment, label: "Commentaire (non visible au public)" %>
    <%= f.submit class:"btn-grad-black", value: "Ajouter film" %>
  <% end %>
</div>
